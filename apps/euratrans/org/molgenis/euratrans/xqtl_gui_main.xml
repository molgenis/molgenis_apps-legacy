<!--
Special GUI file for deploy of EURATRANS on EBI hardware.
A shell script will take this file and copy it over the xqtl_gui_main.xml
in the vanilla xQTL app right before generation.
-->
<molgenis name="vg/euratrans-xgap/xqtl" label="EURATRANS xQTL workbench">
	<plugin name="MolgenisHeader"
		type="org.molgenis.xgap.xqtlworkbench.header.MolgenisHeader" />
	<plugin name="FeedbackForm" type="org.molgenis.feedback.FeedbackForm" />
	<menu name="main">
		<plugin label="Home" name="ClusterDemo" type="plugins.cluster.demo.ClusterDemo" />
		<include file="apps/xqtl/org/molgenis/xqtl/gui/xqtl_gui_wrapper.xml" />
	</menu>
</molgenis>