/*interface relationships*/
        edge [
                arrowhead = "empty"
                color = "#808080"
        ]

/*inheritance relationships*/
        edge [
                arrowhead = "empty"
                color = "black"
        ]
        
        "HemoSample" -> "ObservationTarget"
        "HemoGene" -> "ObservableFeature"
        "HemoProbe" -> "ObservableFeature"

/*one to many 'xref' foreign key relationships*/
        edge [
                arrowhead = "open"
                arrowsize = 0.6
        ]
		"HemoSample" -> "HemoSampleGroup" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"HemoProbe" -> "Chromosome" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"HemoProbe" -> "ProbeSet" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"HemoProbe" -> "HemoGene" [
			headlabel = "0..1"
			taillabel = "*"
		]
        
        
/*many to many 'mref' foreign key relationships*/
        edge [
             arrowtail = "open"
             color = "black"
             headlabel = "*"
             taillabel = "*"
             arrowsize = 0.6
        ]
