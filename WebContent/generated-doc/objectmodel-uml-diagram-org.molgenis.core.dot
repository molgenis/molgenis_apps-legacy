		outputorder="edgesfirst"

        node [
			fontname = "Arial"
            fontsize = 8
            shape = "record"
            color = "#808080"
            style="filled"
            fillcolor = "white"
            layer = "2"
            colorscheme = pastel19
        ]

        edge [
                fontname = "Bitstream Vera Sans"
                fontsize = 8
                layer = "1"
        ]

