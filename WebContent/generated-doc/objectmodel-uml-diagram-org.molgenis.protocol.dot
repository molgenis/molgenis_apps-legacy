/*interface relationships*/
        edge [
                arrowhead = "empty"
                color = "#808080"
        ]

/*inheritance relationships*/
        edge [
                arrowhead = "empty"
                color = "black"
        ]
        
        "ProtocolDocument" -> "MolgenisFile"
        "Workflow" -> "Protocol"

/*one to many 'xref' foreign key relationships*/
        edge [
                arrowhead = "open"
                arrowsize = 0.6
        ]
		"Protocol" -> "Investigation" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Protocol" -> "OntologyTerm" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Protocol" -> "OntologyTerm" [
			headlabel = "0..1"
