        "MolgenisUser" -> "Person"

/*one to many 'xref' foreign key relationships*/
        edge [
                arrowhead = "open"
                arrowsize = 0.6
        ]
		"Person" -> "Institute" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Person" -> "OntologyTerm" [
			headlabel = "0..1"
			taillabel = "*"
