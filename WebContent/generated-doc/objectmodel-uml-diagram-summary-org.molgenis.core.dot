/*inheritance relationships*/
        edge [
                arrowhead = "empty"
                color = "black"
        ]

/*interface relationships*/
        edge [
                color = "#808080"
        ]
    	"Nameable" -> "Identifiable"
    	"OntologyTerm" -> "Nameable"
    	"Ontology" -> "Nameable"
    	"MolgenisFile" -> "Nameable"
    	"RuntimeProperty" -> "Nameable"
    	"Publication" -> "Nameable"
    	"MolgenisEntity" -> "Identifiable"

/*one to many 'xref' foreign key relationships*/
        edge [
                arrowhead = "open"
                arrowsize = 0.6
                color = "black"
        ]
		"OntologyTerm" -> "Ontology" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Publication" -> "OntologyTerm" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Publication" -> "OntologyTerm" [
			headlabel = "0..1"
			taillabel = "*"
		]
		"Publication" -> "OntologyTerm" [
			headlabel = "0..1"
			taillabel = "*"
		]
        
        
/*many to many 'mref' foreign key relationships*/
        edge [
             arrowtail = "open"
             color = "black"
             headlabel = "*"
             taillabel = "*"
             arrowsize = 0.6
             constraint = false
        ]
